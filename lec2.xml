        </title>
        <slides environment="" chapter_type="">
          <slide environment="" chapter_type="Chapter" canon_num="3" scope="section" id="s3" slide="3" course="MATH 1030" chapter="2" section="1" subsection="" subsubsection="" chapter_title="Geometric interpretation of linear equations" section_title="Intersections between Lines and Planes">
            <keywords slide="3" environ="slide" chapter="2" course="MATH 1030"/>
            <title environment="title" chapter_type="Chapter" wbtag="title" scope="section" course="MATH 1030" chapter="2" section="1" subsection="" subsubsection="" serial="" slide="3">
              <paragraphs course="MATH 1030" chapter="2" chapter_type="" section="1" subsection="" subsubsection="" environment="title" wbtag="paragraphs">Intersections between Lines and Planes</paragraphs>
