            <slide environment="" chapter_type="Chapter" wbtag="slide" canon_num="8" id="s8" slide="8" course="MATH 1030" chapter="2" section="1" subsection="2" subsubsection="" chapter_title="Geometric interpretation of linear equations" section_title="Intersections between Lines and Planes">
              <keywords slide="8" environ="slide" chapter="2" course="MATH 1030">
