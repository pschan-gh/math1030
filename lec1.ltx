            <Math tex="\displaystyle x_{1}+2x_{3}-3x_{4}=-1" text="(x _ 1 + 2 * x _ 3) - 3 * x _ 4 = - 1" xml:id="S4.Ex122.m3" fragid="S4.Ex122.m3"><m:math alttext="\displaystyle x_{1}+2x_{3}-3x_{4}=-1" display="inline"><m:mrow><m:mrow><m:mrow><m:msub><m:mi mathsize="120%">x</m:mi><m:mn mathsize="120%">1</m:mn></m:msub><m:mo mathsize="120%" stretchy="false">+</m:mo><m:mrow><m:mn mathsize="120%">2</m:mn><m:mo>⁢</m:mo><m:msub><m:mi mathsize="120%">x</m:mi><m:mn mathsize="120%">3</m:mn></m:msub></m:mrow></m:mrow><m:mo mathsize="120%" stretchy="false">-</m:mo><m:mrow><m:mn mathsize="120%">3</m:mn><m:mo>⁢</m:mo><m:msub><m:mi mathsize="120%">x</m:mi><m:mn mathsize="120%">4</m:mn></m:msub></m:mrow></m:mrow><m:mo mathsize="120%" stretchy="false">=</m:mo><m:mrow><m:mo mathsize="120%" stretchy="false">-</m:mo><m:mn mathsize="120%">1</m:mn></m:mrow></m:mrow></m:math></Math>
            <MathBranch>
              <td align="right"><Math mode="inline" tex="\displaystyle x_{1}+2x_{3}-3x_{4}" text="(x _ 1 + 2 * x _ 3) - 3 * x _ 4" xml:id="S4.Ex122.m1" fragid="S4.Ex122.m1"><m:math alttext="\displaystyle x_{1}+2x_{3}-3x_{4}" display="inline"><m:mrow><m:mrow><m:msub><m:mi mathsize="120%">x</m:mi><m:mn mathsize="120%">1</m:mn></m:msub><m:mo mathsize="120%" stretchy="false">+</m:mo><m:mrow><m:mn mathsize="120%">2</m:mn><m:mo>⁢</m:mo><m:msub><m:mi mathsize="120%">x</m:mi><m:mn mathsize="120%">3</m:mn></m:msub></m:mrow></m:mrow><m:mo mathsize="120%" stretchy="false">-</m:mo><m:mrow><m:mn mathsize="120%">3</m:mn><m:mo>⁢</m:mo><m:msub><m:mi mathsize="120%">x</m:mi><m:mn mathsize="120%">4</m:mn></m:msub></m:mrow></m:mrow></m:math></Math></td>
              <td align="left"><Math mode="inline" tex="\displaystyle=-1" text="absent = - 1" xml:id="S4.Ex122.m2" fragid="S4.Ex122.m2"><m:math alttext="\displaystyle=-1" display="inline"><m:mrow><m:mi/><m:mo mathsize="120%" stretchy="false">=</m:mo><m:mrow><m:mo mathsize="120%" stretchy="false">-</m:mo><m:mn mathsize="120%">1</m:mn></m:mrow></m:mrow></m:math></Math></td>
