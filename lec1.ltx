            <Math tex="\displaystyle{}=" text="=" xml:id="S4.Ex147Xa.m8" fragid="S4.Ex147Xa.m8"><m:math alttext="\displaystyle{}=" display="inline"><m:mo mathsize="120%" stretchy="false">=</m:mo></m:math></Math>
            <MathBranch>
