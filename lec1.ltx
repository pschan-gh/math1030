            <MathBranch>
              <td align="right"><Math tex="\displaystyle x_{5}" text="x _ 5" xml:id="S4.Ex147Xb.m8" fragid="S4.Ex147Xb.m8"><m:math alttext="\displaystyle x_{5}" display="inline"><m:msub><m:mi mathsize="120%">x</m:mi><m:mn mathsize="120%">5</m:mn></m:msub></m:math></Math></td>
              <td align="left"><Math tex="\displaystyle{}=" text="=" xml:id="S4.Ex147Xb.m9" fragid="S4.Ex147Xb.m9"><m:math alttext="\displaystyle{}=" display="inline"><m:mo mathsize="120%" stretchy="false">=</m:mo></m:math></Math></td>
            </MathBranch>
          </MathFork>
          <MathFork>
            <Math tex="\displaystyle 5" text="5" xml:id="S4.Ex147Xb.m10" fragid="S4.Ex147Xb.m10"><m:math alttext="\displaystyle 5" display="inline"><m:mn mathsize="120%">5</m:mn></m:math></Math>
            <MathBranch>
              <td align="right"><Math tex="\displaystyle 5" text="5" xml:id="S4.Ex147Xb.m11" fragid="S4.Ex147Xb.m11"><m:math alttext="\displaystyle 5" display="inline"><m:mn mathsize="120%">5</m:mn></m:math></Math></td>
